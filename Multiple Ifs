	##2. Multiple ifs
	print("Welcometotherollercoaster!")
	height=int(input("Whatisyourheightincm?"))
	bill=0
	
	ifheight>=120:
	print("Youcanridetherollercoaster")
	age=int(input("Whatisyourage?"))
	ifage<12:
	print("Pleasepay$5.")
	bill=5
	elifage<=18:
	print("Pleasepay$7.")
	bill=7
	else:
	bill=12
	print("Pleasepay$12.")
	
	wants_photo=input("Doyouwantaphototaken?YorN.")
	ifwants_photo=="y":
	bill+=3
	
	print(f"You'retotallids${bill}")
	
	else:
	print("Sorryyouhavetogrowtallerbeforeyoucanride.")
